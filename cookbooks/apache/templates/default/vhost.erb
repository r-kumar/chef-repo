# vhost template file
#NameVirtualHost 192.168.1.116:80
<VirtualHost *:<%= @port %>>

	ServerName <%= @domain %>
	DocumentRoot <%= @document_root %>

  <Directory />
   Options FollowSymLinks
   AllowOverride None
   Require all granted
  </Directory>

  <Directory <%= @document_root%> >
    Options Indexes FollowSymLinks MultiViews
    AllowOverride None
   # Order allow,deny
   # allow from all
   Require all granted
  </Directory>

</VirtualHost>
